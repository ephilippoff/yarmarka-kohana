{% extends "layouts/user" %}


{% block css %} {{ parent() }} {% endblock css%}
{% block js %} {% endblock js %}

{% block title %} {{ seo_attributes.title }} {% endblock title%}
{% block meta_description %}{% endblock meta_description%}
{% block crumbs %} {% endblock crumbs %}

{% block header %} 
	{{ requestblock('block_twig/topline', {'favourites':favourites}) }}
	{% include 'block/header/logoline' %}
	{% include 'search/user/crumbs' %}
	{% include 'search/user/company/promo' %}
		
{% endblock header %} 

{% block subcategories %}{% endblock subcategories %}

{% block leftside %}
	{% if user_categories|length > 0 %}
		{% include 'search/user/company/usercategories' %}
	{% endif %}
{% endblock leftside %}

{% block content %}
	{% if (main_search_result|length) %}
		{% include 'search/user/company/result' %}
		{% include 'search/paginator' %}
	{% else %}
		{{empty_text}}
	{% endif %}
{% endblock content%}


{% block footertext %} {% endblock footertext %}

{% block footer %} 
	{{ parent() }}
{% endblock footer %}


{%block page_name%}usersearch{% endblock page_name %}