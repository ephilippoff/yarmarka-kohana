{% set mini_cart_path = 'detail/mini/default_tablerow' %}
{% if check_access("object.moderate") %}
	{% set mini_cart_path = 'detail/mini/default_tablerow_for_admin' %}
{% endif %}

{% if premium_search_result|length > 0 %}
	{% for object in premium_search_result %}
		{% include mini_cart_path %}
	{% endfor %}
{% endif %}


{% for object in main_search_result if object.compiled.url %}

	{% if loop.index == 3 %}
		<ins class="adsbygoogle"
			style="display:inline-block;width:100%;height:90px"
			data-ad-client="ca-pub-4714092941447146"
			data-ad-slot="1610833118"></ins>
		<script>
		(adsbygoogle = window.adsbygoogle || []).push({});
		</script>
	{% endif %}

	{% if loop.index in banner_zone_positions|keys %}
	<div class="banner {zoneID: {{banner_zone_positions[loop.index]}} }"></div>
	{% endif %}

	{% include mini_cart_path %}

	

{% else %}
	{% include 'search/noresult' %}
{% endfor %}