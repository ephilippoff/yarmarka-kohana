
<div class="clearfix">
	
	
	<div class="control_buttons module fs0 fl options-wrap clearfix">
		<span class="span-link menu-item js-search-extend control-item" onclick="mobile(function(){$('.options-wrap').fadeOut();});">Расширенный поиск</span>
		{% if not isGuest %}
		<div class="menu-item" data-role="subscription-module"><span class="span-link">Подписка на обновление</span></div>
		{% else %}
		<a class="menu-item red" href="/user/login?return={{ strim(get_request_uri(), '/') }}"  rel="nofollow">Подписка на обновление</a>
		{% endif %}
		<div class="menu-item span-link" id="limit"><i class="fa fa-list"></i> {{limitList}}</div>
	</div>

	<div class="fr small_pag hidden-xxs hidden-xs">
		{{small_pagination}}
	</div>

</div>


<div id="save-search-cont"></div>

